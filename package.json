    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.4",
    "jsonwebtoken": "^9.0.3",
    "next": "^15.5.12",
    "openai": "^6.18.0",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
